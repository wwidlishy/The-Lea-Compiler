## Just A wrapper for The C Standard Library ngl

asm("extern printf", "linux")
asm("extern _printf", "windows")

asm("extern exit", "linux")
asm("extern _exit", "windows")

echo Void 1:: String
does
    asm("call printf", "linux")
    asm("call _printf", "windows")
end

exit Void 1 :: Integer
does
    asm("call exit", "linux")
    asm("call _exit", "windows")
end